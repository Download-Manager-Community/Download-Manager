{
  "name": "Download Manager",
  "version": "1.0.1.0",
  "manifest_version": 3,
  "description": "Download Manager browser extension to download files.",
  "icons": {
    "16": "icons/icon.png",
    "32": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "permissions":[
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background":{
    "service_worker": "scripts/background.js"
  }
}